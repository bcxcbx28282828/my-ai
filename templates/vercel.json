{
  "version": 2,
  "builds": [
    { "src": "main.py", "use": "@vercel/python" },
    { "src": "templates/index.html", "use": "@vercel/static" },
    { "src": "static/script.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/chat", "dest": "main.py" },
    { "src": "/(.*)", "dest": "/templates/index.html" }
  ]
}
